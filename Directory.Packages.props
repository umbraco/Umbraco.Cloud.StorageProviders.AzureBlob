<Project>
  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
  </PropertyGroup>
  <ItemGroup>
    <GlobalPackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" />
    <GlobalPackageReference Include="Nerdbank.GitVersioning" Version="3.6.133" />
    <GlobalPackageReference Include="StyleCop.Analyzers" Version="1.2.0-beta.507" />
    <GlobalPackageReference Include="Umbraco.Code" Version="2.0.0" />
    <GlobalPackageReference Include="Umbraco.GitVersioning.Extensions" Version="0.2.0" />
  </ItemGroup>
  <PropertyGroup>
    <UmbracoCmsPackageVersion>[12.0.0, 13)</UmbracoCmsPackageVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageVersion Include="Umbraco.StorageProviders.AzureBlob" Version="$(UmbracoCmsPackageVersion)" />
    <PackageVersion Include="Umbraco.StorageProviders.AzureBlob.ImageSharp" Version="$(UmbracoCmsPackageVersion)" />
    <PackageVersion Include="Umbraco.StorageProviders.AzureBlob.ImageSharp2" Version="$(UmbracoCmsPackageVersion)" />
  </ItemGroup>
</Project>
